<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="member">
    <select id="sel" resultType="wooyoung.blog.dto.HomeDto">
        SELECT * FROM blog.member;
    </select>

    <insert id="ins" parameterType="hashMap">
        INSERT INTO blog.member
        VALUES (#{name}, #{phone});
    </insert>
</mapper>